    "next-compose-plugins": "^2.2.0",
    "next-routes": "^1.4.2",
    "next-seo": "^3.2.0",
    "next-transpile-modules": "^2.3.1",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
