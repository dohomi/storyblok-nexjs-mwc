  "scripts": {
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node server.js",
