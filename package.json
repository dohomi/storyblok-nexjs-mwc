    "js-cookie": "2.2.1",
    "marked": "1.0.0",
    "match-sorter": "4.1.0",
    "mdi-material-ui": "6.14.0",
    "next": "9.3.6",
    "next-compose-plugins": "2.2.0",
    "next-routes": "1.4.2",
    "next-seo": "4.4.0",
    "next-transpile-modules": "3.3.0",
    "node-fetch": "2.6.0",
