  "description": "Next.js Storyblok Boilerplate",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "__dev": "next",
    "dev": "nodemon",
    "_dev": "node server.ts",
    "build": "next build",
    "start": "node server/server.ts",
    "_start": "NODE_ENV=production node server.ts",
    "deploy": "NODE_ENV=production now",
    "server": "micro-dev src/api/forwardMail.js -p=4444",
    "get-storyblok-schema": "storyblok pull-components --space=57415",
    "gen-ts-schema": "node generate-ts-schema.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "rm -rf public_storybook && build-storybook -c .storybook -o public_storybook && cd public_storybook && now"
  },
