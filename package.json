{
  "name": "@lumen/mwc",
  "version": "1.0.0",
  "description": "Next.js Storyblok Boilerplate",
  "main": "index.js",
  "scripts": {
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next",
    "dev1": "next -p 3001",
    "dev2": "next -p 3002",
    "dev3": "next -p 3003",
    "dev4": "next -p 3004",
    "__dev": "nodemon",
    "__dev2": "PORT=3001 nodemon",
    "__dev3": "PORT=3002 nodemon",
    "__dev4": "PORT=3003 nodemon",
    "_dev": "node server.ts",
    "build": "next build",
    "__build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production next start",
    "__start": "NODE_ENV=production node server/server.ts",
    "deploy": "NODE_ENV=production now",
    "server": "micro-dev src/api/forwardMail.js -p=4444",
    "gen-ts-schema": "node generate-ts-schema.js",
    "fetch-storyblok-components": "storyblok pull-components --space=66717",
    "update-storyblok": "yarn fetch-storyblok-components && yarn gen-ts-schema && yarn codegen",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "rm -rf public_storybook && build-storybook -c .storybook -o public_storybook && cd public_storybook && now",
    "build-prod": "rm -rf dist && tsc -p tsconfig.build.json",
    "build-prod-declare": "rm -rf dist && tsc --declaration module.js.ts tsconfig.build.json"
  },
  "author": "Dominic Garms <djgarms@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/js-cookie": "^2.2.3",
    "@types/marked": "^0.7.0",
    "@types/node": "^12.12.8",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.11",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/ua-parser-js": "^0.7.33",
    "@u-wave/react-vimeo": "^0.8.1",
    "clsx": "^1.0.4",
    "css-mediaquery": "^0.1.2",
    "fontfaceobserver": "^2.1.0",
    "intersection-observer": "^0.7.0",
    "js-cookie": "^2.2.1",
    "marked": "^0.7.0",
    "modern-normalize": "^0.5.0",
    "next": "^9.1.4",
    "next-compose-plugins": "^2.2.0",
    "next-routes": "^1.4.2",
    "next-seo": "^2.1.2",
    "next-transpile-modules": "^2.3.1",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hooks-global-state": "^0.16.0",
    "react-inlinesvg": "^1.1.7",
    "react-intersection-observer": "^8.24.1",
    "react-player": "^1.14.0",
    "react-scroll-parallax": "^2.2.0",
    "react-swipeable-views": "^0.13.3",
    "react-swipeable-views-utils": "^0.13.3",
    "sitemap": "^5.0.1",
    "storyblok-js-client": "^2.0.9",
    "storyblok-react": "^0.0.10",
    "ts-node": "^8.5.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "ua-parser-js": "^0.7.20",
    "unfetch": "^4.1.0",
    "use-debounce": "^3.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.1.3",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-document-nodes": "^1.8.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-app": "^9.0.2",
    "css-loader": "^3.2.0",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "graphql": "^14.5.8",
    "json-schema-to-typescript": "^7.1.0",
    "next-offline": "^4.0.6",
    "nodemon": "^1.19.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-docgen-typescript-loader": "^3.4.0",
    "storybook-addon-smart-knobs": "^6.0.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
