{
  "name": "@lumen/mwc",
  "version": "1.0.0",
  "description": "Next.js full website kit powered by Storyblok",
  "main": "index.js",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=true npm run build",
    "dev": "next",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "deploy": "NODE_ENV=production now",
    "server": "micro-dev src/api/forwardMail.js -p=4444",
    "gen-ts-schema": "node generate-ts-schema.js",
    "fetch-storyblok-components": "storyblok pull-components --space=66717",
    "update-storyblok": "npm run fetch-storyblok-components && npm run gen-ts-schema && npm run codegen",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "deploy-storybook": "rm -rf public_storybook && build-storybook -c .storybook -o public_storybook && cd public_storybook && now --prod"
  },
  "author": "Dominic Garms <djgarms@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": "12.x"
  },
