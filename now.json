{
  "version": 2,
  "name": "storyblok-bare-mwc",
  "regions": [
    "bru1"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "^/(favicon.ico|robots.txt)$",
      "headers": {
        "Cache-Control": "public, s-maxage=31536000, max-age=31536000, immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "^/_next/static/(.*)$",
      "headers": {
        "Cache-Control": "public, s-maxage=31536000, max-age=31536000, immutable"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/index?slug=$1",
      "headers": {
        "Cache-Control": "public, s-maxage=0, max-age=0"
      }
    },
    {
      "src": "/(.*)$",
      "dest": "/index?slug=$1",
      "headers": {
        "Cache-Control": "public, s-maxage=0, max-age=0"
      }
    }
  ]
}
