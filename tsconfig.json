{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "esnext",
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "baseUrl": "./",
    "rootDir": "./src",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "jsx": "react",
    "paths": {
      "@": [
        "./"
      ],
      "*": [
        "src/*",
        "node_modules/*"
      ],
      "@components": [
        "./src/components/ComponentRender.tsx"
      ],
      "@polyfills": [
        "./client/polyfills.js"
      ],
      "@initialData/*": [
        "./src/utils/initial-props/*"
      ]
    },
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    ".next"
  ],
  "include": [
    "next-env",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
