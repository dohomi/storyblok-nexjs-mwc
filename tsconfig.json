{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./components/*"
      ],
      "fonts": [
        "./components/fonts.ts"
      ],
      "routes": [
        "./server/routes.ts"
      ],
      "client": [
        "./client"
      ]
    }
  },
  "exclude": [
    "node_modules",
    ".next"
  ],
  "include": [
    "next-env",
    "**/*.ts",
    "**/*.tsx"
  ]
}
